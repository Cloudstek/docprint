{
  "parser": "babel-eslint",
  "extends": "xo",
  "env": {
    "node": true,
    "browser": false
  },
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-prototype-builtins": "off",
    "flowtype-errors/show-errors": 2
  },
  "plugins": [
    "flowtype-errors"
  ]
}
